msgid "Usage:"
msgstr "Utilisation:"

#: tazpkg:115
msgid "tazpkg [command] [package|dir|pattern|list|cat|--opt] [dir|--opt]"
msgstr "tazpkg [commande] [paquet|rép|motif|liste|cat|--opt] [rép|--opt]"

#: tazpkg:116
msgid "SHell:"
