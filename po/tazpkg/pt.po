#: tazpkg:88
msgid "SliTaz package manager - Version"
msgstr "Gerenciador de Pacotes Tazpkg - Versão"

#: tazpkg:89
msgid "Usage"
msgstr "Uso"

#: tazpkg:89
msgid "tazpkg [command] [package|dir|pattern|list|cat|--opt] [dir|--opt]"
msgstr ""
"tazpkg [comando] [pacote|diretório|padrão|lista|categoria|--opt] "
"[diretório|--opt]"

#: tazpkg:91
msgid "Commands"
