#: tazpkg:86
msgid "SliTaz package manager - Version"
msgstr "Gestionnaire de paquets SliTaz - Version"

#: tazpkg:87
msgid "Usage"
msgstr "Utilisation"

#: tazpkg:87
msgid "tazpkg [command] [package|dir|pattern|list|cat|--opt] [dir|--opt]"
msgstr "tazpkg [commande] [paquet|rép|motif|liste|cat|--opt] [rép|--opt]"

#: tazpkg:89
msgid "Commands"
