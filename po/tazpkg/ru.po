#, sh-format
msgid "Missing: $MISSING_PACKAGE"
msgstr "Отсутствует: $MISSING_PACKAGE"

#: tazpkg:759
#, sh-format
msgid "$deps missing package to install."
msgid_plural "$deps missing packages to install."
msgstr[0] "Отсутствует $deps пакет для установки."
msgstr[1] "Отсутствуют $deps пакета для установки."
msgstr[2] "Отсутствуют $deps пакетов для установки."

#: tazpkg:776
msgid "Install all missing dependencies"
msgstr "Установить все отсутствующие зависимости"

