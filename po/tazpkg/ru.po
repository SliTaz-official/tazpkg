msgid_plural "<c 32>$files</c> files installed with $PACKAGE"
msgstr[0] "<c 32>$files</c> файл, устанавливаемый с $PACKAGE."
msgstr[1] "<c 32>$files</c> файла, устанавливаемых с $PACKAGE."
msgstr[2] "<c 32>$files</c> файлов, устанавливаемых с $PACKAGE."

